{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Sdk",
  "extends": "./base.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "target": "es2019",
    "lib": ["es2022", "dom"]
  }
}
